<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".REGISTRAR_RESTAURANTE">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:scaleType="centerCrop"
        app:srcCompat="@drawable/casa_interior_bob_esponja"
        tools:layout_editor_absoluteX="-82dp"
        tools:layout_editor_absoluteY="0dp" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="369dp"
        android:layout_height="148dp"
        android:background="#d8000000"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.692"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.827">

        <ImageButton
            android:id="@+id/atrasVR3"
            android:layout_width="53dp"
            android:layout_height="44dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="143dp"
            android:layout_marginEnd="-2dp"
            android:layout_marginRight="-2dp"
            android:background="?attr/actionModeCloseDrawable" />

        <com.google.android.gms.maps.MapView
            android:id="@+id/MapView1"
            android:layout_width="354dp"
            android:layout_height="129dp"
            android:layout_alignParentStart="false"
            android:layout_alignParentEnd="false"
            android:layout_marginStart="26dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="36dp"
            tools:ignore="MissingConstraints" />

    </RelativeLayout>

    <EditText
        android:id="@+id/namerestorant"
        android:layout_width="273dp"
        android:layout_height="39dp"
        android:layout_marginTop="40dp"
        android:background="@android:drawable/screen_background_light_transparent"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:paddingTop="9dp"
        android:paddingRight="20dp"
        android:textColor="@color/colorAccent2"
        android:textColorHint="#474444"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registrarrestautanteext"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/nit"
        android:layout_width="273dp"
        android:layout_height="39dp"
        android:layout_marginTop="16dp"
        android:background="@android:drawable/screen_background_light_transparent"
        android:ems="10"
        android:hint="NIT"
        android:inputType="numberDecimal"
        android:paddingLeft="10dp"
        android:paddingTop="9dp"
        android:paddingRight="20dp"
        android:textColor="@color/colorAccent2"
        android:textColorHint="#474444"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namerestorant"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/propietario"
        android:layout_width="273dp"
        android:layout_height="39dp"
        android:layout_marginTop="15dp"
        android:background="@android:drawable/screen_background_light_transparent"
        android:ems="10"
        android:hint="Propietario"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:paddingTop="9dp"
        android:paddingRight="20dp"
        android:textColor="@color/colorAccent2"
        android:textColorHint="#474444"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nit"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/phonerestorant"
        android:layout_width="273dp"
        android:layout_height="39dp"
        android:layout_marginTop="15dp"
        android:background="@android:drawable/screen_background_light_transparent"
        android:ems="10"
        android:hint="Telefono"
        android:inputType="numberDecimal"
        android:paddingLeft="10dp"
        android:paddingTop="9dp"
        android:paddingRight="20dp"
        android:textColor="@color/colorAccent2"
        android:textColorHint="#474444"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.463"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/propietario"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/streetrestorant"
        android:layout_width="281dp"
        android:layout_height="36dp"
        android:background="@android:drawable/screen_background_light_transparent"
        android:ems="10"
        android:hint="Calle"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:paddingTop="9dp"
        android:paddingRight="20dp"
        android:textColor="@color/colorAccent2"
        android:textColorHint="#474444"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phonerestorant"
        app:layout_constraintVertical_bias="0.046" />

    <Button
        android:id="@+id/crear"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:text="Crear Restaurant"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/registrarrestautanteext"
        android:layout_width="317dp"
        android:layout_height="35dp"
        android:text="REGISTRAR RESTAURANTE"
        android:textColor="@color/cardview_light_background"
        android:textSize="20sp"
        app:fontFamily="@font/abril_fatface"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.829"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <ImageView
        android:id="@+id/fotoperfil2"
        android:layout_width="64dp"
        android:layout_height="67dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/nit"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageButton
        android:id="@+id/atrasRRA"
        android:layout_width="65dp"
        android:layout_height="66dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/ic_baseline_subdirectory_arrow_left_24"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.989" />

    <EditText
        android:id="@+id/idcliente"
        android:layout_width="270dp"
        android:layout_height="2dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rectangle"
        android:ems="10"
        android:hint="Nombre"
        android:inputType="textPersonName"
        android:paddingLeft="10dp"
        android:paddingTop="9dp"
        android:paddingRight="20dp"
        android:textColor="@color/colorAccent2"
        android:textColorHint="#ccc"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/namerestorant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/registrarrestautanteext" />

</androidx.constraintlayout.widget.ConstraintLayout>